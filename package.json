  "private": true,
  "license": "JRL",
  "description": "Today in King Gizzard History",
  "author": "Axe <alxndr+git-kglw-today@gmail.com",
  "keywords": [
    "music",
    "kglw",
    "gatsby"
  ],
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build-storybook": "storybook build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "data:updateShows": "node updateShows.mjs",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "eslint . && prettier --check .",
    "prestart": "npm run data:updateShows",
    "preview": "vite preview",
    "start": "npm run dev",
    "storybook": "storybook dev -p 6006",
    "test": "npm run test:unit -- --run && npm run test:integration",
    "test:integration": "cypress run",
    "test:serve": "start-server-and-test http://localhost:8000 'npm run test:integration -- --config baseUrl=http://localhost:8000'",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@storybook/addon-essentials": "^8.4.5",
    "@storybook/addon-interactions": "^8.4.5",
    "@storybook/addon-svelte-csf": "^5.0.0-next.11",
    "@storybook/blocks": "^8.4.5",
    "@storybook/svelte": "^8.4.5",
    "@storybook/sveltekit": "^8.4.5",
    "@storybook/test": "^8.4.5",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.16.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.6",
    "start-server-and-test": "^2.0.8",
    "storybook": "^8.4.5",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.0.3",
    "vitest": "^2.0.4"
  }
}
