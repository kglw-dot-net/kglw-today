{
  "name": "kglw.today",
  "version": "1.0.0",
  "private": true,
  "description": "kglw.today",
  "author": "Alexander",
  "keywords": [],
  "scripts": {
    "prebuild": "npm run data:updateShows",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "data:updateShows": "node -e '(async() => process.stdout.write(JSON.stringify((await (await fetch(\"https://kglw.net/api/v1/shows.json\")).json()).data,null,2)+\"\\n\"))()' >src/data/shows.json",
    "predevelop": "npm run data:updateShows",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "prestart": "npm run data:updateShows",
    "start": "gatsby develop"
  },
  "dependencies": {
    "gatsby": "^5.11.0",
    "gatsby-plugin-sass": "^6.8.0",
    "gatsby-plugin-sitemap": "^6.8.0",
    "gatsby-source-filesystem": "^5.8.0",
    "gatsby-transformer-json": "^5.8.0",
    "gatsby-transformer-remark": "^6.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.60.0"
  },
  "devDependencies": {
    "pretty-format": "^29.5.0"
  }
}
